const getValidationErrorMessage = /* istanbul ignore next */exports.getValidationErrorMessage = (auth, fieldID) => {

  if (auth && auth.error && auth.error.code && auth.error.code.indexOf(fieldID) > 0) {
    return auth.error.message;
  }

  return undefined;
};

