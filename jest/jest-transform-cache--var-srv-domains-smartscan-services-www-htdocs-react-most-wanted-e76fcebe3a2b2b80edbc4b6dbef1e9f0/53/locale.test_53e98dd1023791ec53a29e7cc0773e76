describe('locale', () => {
  it('should create an action to add a todo', () => {
    const loc = 'de';
    const expectedAction = {
      type: locale.UPDATE_LOCALE,
      loc };

    expect(locale.updateLocale(loc)).toEqual(expectedAction);
  });
});